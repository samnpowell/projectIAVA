        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}
try{
var status = &apos;{!Campaign.WE_FW8_NP__Wealth_Engine_Batch_Status__c}&apos;;
if( status == &quot;Processing&quot; || status == &quot;Salesforce Batch In Progress&quot;){
var alertMsg = &quot;This Campaign is already submitted and in progress. Please check back after some time.&quot;;
alert(alertMsg);
} else {
var campaignMemberRecords = sforce.connection.query(&quot;SELECT Id FROM CampaignMember WHERE CampaignId = &apos;{!Campaign.Id}&apos;&quot;);
if (campaignMemberRecords.size != 0) {
if (window.confirm(&quot;Are you sure you want to process &quot; + campaignMemberRecords.size + &quot; records?  This may take some time.  Check back later to see results&quot;)) {
var message = sforce.apex.execute(&quot;WE_FW8_NP.PersonSearchWebService&quot;,&quot;searchPersonWebService&quot;,{campaignId:&quot;{!Campaign.Id}&quot;});
if (message != &apos;&apos;) {
alert(message);
}
}
} else {
alert(&quot;No Campaign Members for this record&quot;);
}
}
} catch(err) {
alert(err);
}</url>
