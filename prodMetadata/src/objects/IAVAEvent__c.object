    <validationRules>
        <fullName>End_Time_validate</fullName>
        <active>true</active>
        <description>Validate that format of End Time is like: 12:15 PM</description>
        <errorConditionFormula>AND(NOT(ISBLANK(End_Time__c)),NOT(REGEX(End_Time__c, &quot;^ *(1[0-2]|[1-9]):[0-5][0-9] *(a|p|A|P)(m|M) *$&quot;)))</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>That is not a valid End Time.  The correct format should be: 12:15 PM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Time_validate</fullName>
        <active>true</active>
        <description>Validate that format of Start Time is like: 5:15 PM</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Start_Time__c)),NOT(REGEX(Start_Time__c, &quot;^ *(1[0-2]|[1-9]):[0-5][0-9] *(a|p|A|P)(m|M) *$&quot;)))</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>That is not a valid Start Time.  The correct format should be: 12:15 PM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_code_validate</fullName>
        <active>true</active>
        <errorConditionFormula>and (
  not(isblank(Location_Zip_Code__c)),

 or(
   len( Location_Zip_Code__c ) &lt;&gt; 5
 , not( isnumber(Location_Zip_Code__c ))
 )
)</errorConditionFormula>
        <errorDisplayField>Location_Zip_Code__c</errorDisplayField>
        <errorMessage>Zip code should be a 5-digit number</errorMessage>
    </validationRules>
