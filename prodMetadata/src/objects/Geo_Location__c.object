    <label>Geo Location</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Geocode_Status__c</columns>
        <columns>Icon_URL__c</columns>
        <columns>lat__c</columns>
        <columns>lon__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>Created_Today</fullName>
        <columns>NAME</columns>
        <columns>Geocode_Status__c</columns>
        <columns>Icon_URL__c</columns>
        <columns>lat__c</columns>
        <columns>lon__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CREATED_DATE</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </filters>
        <label>Created Today</label>
    </listViews>
    <listViews>
        <fullName>Locations_w_Zip_Lat_Lon</fullName>
        <columns>NAME</columns>
        <columns>lat__c</columns>
        <columns>lon__c</columns>
        <filterScope>Everything</filterScope>
        <label>Locations w/ Zip, Lat, Lon</label>
    </listViews>
    <listViews>
        <fullName>Unsuccessful</fullName>
        <columns>NAME</columns>
        <columns>Geocode_Status__c</columns>
        <columns>WebLink__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Geocode_Status__c</field>
            <operation>notEqual</operation>
            <value>G_GEO_MANUAL,G_GEO_SUCCESS</value>
        </filters>
        <label>Unsuccessful</label>
    </listViews>
    <nameField>
        <label>Geo Location Name</label>
