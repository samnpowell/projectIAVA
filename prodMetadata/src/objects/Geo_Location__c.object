    <pluralLabel>Geo Locations</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>Geocode_Status__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>lat__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>lon__c</customTabListAdditionalFields>
        <searchFilterFields>NAME</searchFilterFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Valid_Lat</fullName>
        <active>true</active>
        <description>ensure that the latitude falls on the map of the earth</description>
        <errorConditionFormula>lat__c &lt; -85.0  || lat__c &gt; 85.0</errorConditionFormula>
        <errorDisplayField>lat__c</errorDisplayField>
        <errorMessage>Valid Latitudes are less than 85.0 North and greater than -85.0 South</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Lon</fullName>
        <active>true</active>
        <description>Valid Longitudes are &gt; -180 East and &lt; 180 West</description>
        <errorConditionFormula>lon__c &gt; 180 || lon__c &lt; -180</errorConditionFormula>
        <errorDisplayField>lon__c</errorDisplayField>
        <errorMessage>Valid Longitudes are greater than -180.0 and less than 180.0</errorMessage>
    </validationRules>
