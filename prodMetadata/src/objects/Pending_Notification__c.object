    <validationRules>
        <fullName>Give_Or_Campaign</fullName>
        <active>true</active>
        <description>Validate that the conditions set for a Give notification are valid for the Give (if specified).  Validate that both the Campaign lookup and the Give lookup are not populated at the same time.</description>
        <errorConditionFormula>OR( AND( ISBLANK( Campaign__c ) , ISBLANK( Give__c ) ) , AND( NOT( ISBLANK( Campaign__c ) ), NOT( ISBLANK( Give__c ) ) ) )</errorConditionFormula>
        <errorMessage>The Give or the campaign are required, but no both of them.</errorMessage>
    </validationRules>
