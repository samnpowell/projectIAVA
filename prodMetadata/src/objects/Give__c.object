    <fields>
        <fullName>Available__c</fullName>
        <externalId>false</externalId>
        <formula>if(and(ispickval(Type__c,&quot;Unlimited Give&quot;),or(ispickval(Status__c,&quot;Open&quot;),ispickval(Status__c,&quot;Waitlist&quot;))),&quot;Available&quot;,
if(TEXT(Status__c) == &quot;Closed Invisible&quot;,&quot;Closed&quot;,
if(TEXT(Status__c) == &quot;Closed Visible&quot;,&quot;Closed&quot;,
if(TEXT(Status__c) == &quot;Waitlist&quot;,&quot;Waitlist&quot;,
if(TEXT(Status__c) == &quot;Open&quot;, 
if(Number_Remaining__c &lt;=0,&quot;Available&quot;,
if(Number_Remaining__c &gt; 0 &amp;&amp; Number_Remaining__c &lt; 25, &apos;ONLY &apos; + TEXT(Number_Remaining__c) +  &apos; AVAILABLE&apos;,
if(Number_Remaining__c &gt;= 25, &quot;25+ Available&quot;,&quot;&quot;
)))
,&quot;&quot;)
))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Available</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
