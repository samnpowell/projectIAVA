    <fields>
        <fullName>Deadline__c</fullName>
        <externalId>false</externalId>
        <formula>if(isblank(Latest_Application_Deadline__c),&quot;&quot;, 
IF(MONTH(Latest_Application_Deadline__c ) == 1,&quot;Jan&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 2,&quot;Feb&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 3,&quot;Mar&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 4,&quot;Apr&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 5,&quot;May&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 6,&quot;Jun&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 7,&quot;Jul&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 8,&quot;Aug&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 9,&quot;Sep&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 10,&quot;Oct&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 11,&quot;Nov&quot;,
(IF(MONTH(Latest_Application_Deadline__c ) == 12,&quot;Dec&quot;,&quot;&quot;))))))))))))))))))))))) &amp; &quot; &quot; &amp; TEXT(DAY(Latest_Application_Deadline__c )) &amp; &quot;, &quot; &amp; TEXT(YEAR(Latest_Application_Deadline__c ))
)</formula>
        <label>Deadline</label>
        <required>false</required>
