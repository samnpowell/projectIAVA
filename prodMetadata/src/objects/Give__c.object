        <externalId>false</externalId>
        <formula>IF ((AND (  AIR_Required__c  = True,  AIR_Completed__c  = False,  Earliest_Start_Date__c  +2 &lt; DATEVALUE(NOW()))), &quot;True&quot;, &quot;False&quot;)</formula>
