        <externalId>false</externalId>
        <formula>IF (NOT (ISBLANK (  Event__c )), ( Event__r.Num_Replies_Yes__c ),
IF (NOT (ISBLANK ( Campaign__c)),  (Campaign__r.Num_Confirmed__c +  Campaign__r.Num_Attended__c ) ,
IF (NOT (ISBLANK ( Give__c )),  Give__r.Number_of_Confirmed_Units__c  , VALUE(&quot;&quot;))))</formula>
