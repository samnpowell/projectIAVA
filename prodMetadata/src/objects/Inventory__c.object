        <fullName>Restock__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(Small_stocked__c &gt; 0, Small_requested__c &gt; 0, Small_remaining__c &lt;= 30, RecordType.Name = &apos;Apparel&apos;), &apos;Small&apos;,
    IF(AND(Medium_stocked__c &gt; 0, Medium_requested__c &gt; 0, Medium_remaining__c &lt;= 30, RecordType.Name = &apos;Apparel&apos;), &apos;Medium&apos;,
    IF(AND(Large_stocked__c &gt; 0, Large_requested__c &gt; 0, Large_remaining__c &lt;= 30, RecordType.Name = &apos;Apparel&apos;), &apos;Large&apos;,
    IF(AND(X_Large_stocked__c &gt; 0, X_Large_requested__c &gt; 0, X_Large_remaining__c &lt;= 30, RecordType.Name = &apos;Apparel&apos;), &apos;X-Large&apos;,
    IF(AND(XX_Large_stocked__c &gt; 0, XX_Large_requested__c &gt; 0, XX_Large_remaining__c &lt;=30, RecordType.Name = &apos;Apparel&apos;), &apos;XX-Large&apos;,
    null)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
